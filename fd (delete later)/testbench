:vpi_time_precision + 0;
:vpi_module "/usr/local/lib/ivl/system.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_sys.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_textio.vpi";
:vpi_module "/usr/local/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/local/lib/ivl/va_math.vpi";
S_0x1554df0 .scope module, "tb" "tb" 2 1;
 .timescale 0 0;
v0x15564a0_0 .var/i "fd", 31 0;
v0x159c2a0_0 .var "str", 360 1;
    .scope S_0x1554df0;
T_0 ;
    %vpi_func 2 6 "$fopen" 32, "my_file.txt", "r" {0 0 0};
    %store/vec4 v0x15564a0_0, 0, 32;
T_0.0 ;
    %vpi_func 2 9 "$feof" 32, v0x15564a0_0 {0 0 0};
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz T_0.1, 8;
    %delay 10, 0;
    %vpi_call 2 12 "$fgets", v0x159c2a0_0, v0x15564a0_0 {0 0 0};
    %vpi_call 2 15 "$display", "%0s", v0x159c2a0_0 {0 0 0};
    %jmp T_0.0;
T_0.1 ;
    %vpi_call 2 17 "$fclose", v0x15564a0_0 {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
